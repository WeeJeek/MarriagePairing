<!--pages/test_description/test_description.wxml-->
<view class="page">
  <view class="header">
      <text class="header-text">{{"盟约吧"}}</text>
    </view>
  <view class = "card">
    <view class="header-title">
      <p class = "title-text">{{test_title}}</p>
    </view>
    <view id = "test-description" class="test-intro">
      {{test_description}}
    </view>
    <view id = "test rule" class="test-intro">
      {{test_rule}}
    </view>
  </view>

  <view>
      <button  class = "button" bindtap="start_tests"> 开始测试 </button>
    </view>
</view>
