/* pages/index/index.wxss */
.page{
  height: 100%;
}
.header {
  display: flex;
  justify-content: center;
  margin-bottom: 50rpx;
}

.header-text {
  font-size: 36rpx;
  font-weight: bold;
  color: #6B584F;
}



.button-container {
  background-color: #F5F2ED;
  padding: 30rpx;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
  box-shadow: 0 5rpx 5rpx #CEB8B1;
}

.button {
  background-color: #CEB8B1;
  border-radius: 10rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #6B584F;
  padding: 20rpx;
  box-shadow: 0 5rpx 5rpx #CEB8B1;
}